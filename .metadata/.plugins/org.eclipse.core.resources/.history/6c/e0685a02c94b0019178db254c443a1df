from random import randint

def GenerarLista():
    M = list()
    for i in range(randint(1,5)):
        M.append(list())
        for j in range(randint(1,5)):
            M[i].append(randint(0,9))
    return M


def Semejantes(m1, m2):
    if len(m1) == len(m2):
        for i in range(len(m1)):
            if len(m1[i] == len(m2[i])):
                semejantes = True
            else:
                semejantes = False
                break
    else:
        semejantes = False
    
    return semejantes
    
print(GenerarLista())
    
    




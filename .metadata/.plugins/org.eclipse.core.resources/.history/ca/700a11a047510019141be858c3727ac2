from random import randint

palabras = {1:'hola', 2:'coche', 3:'bicicleta'}

seleccionada = randint(1,3)
#print(seleccionada)
intentos = 6

i = 0
encontrado = False
while i < intentos and not encontrado:
    print("-"*len(palabras[seleccionada]))
    i += 1
    
    
    



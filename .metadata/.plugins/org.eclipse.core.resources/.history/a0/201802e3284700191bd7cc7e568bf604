package domain;

import main.Caballo;

public class Main {

	public static void main(String[] args) throws InterruptedException {
		int longitudPista = 1000;
		Caballo miCaballo = new Caballo(longitudPista);
		
		miCaballo.start();
		//le digo al main con este m√©todo que espere
		miCaballo.join();
		System.out.println("Terminado");

	}

}

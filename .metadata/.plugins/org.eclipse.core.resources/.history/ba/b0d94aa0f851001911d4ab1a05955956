package domain;

public class Cesto {
	
	//4ยบ creo una variable para saber cuantas manzanas tengo en el cesto en ese momento
	private int manzanasCesto;
	private int capacidadCesto;
	
	//3ยบ creo el constructor de la clase Cesto
	public Cesto (int capacidadCesto){
		this.capacidadCesto = capacidadCesto;
		this.manzanasCesto = capacidadCesto;
	}
}

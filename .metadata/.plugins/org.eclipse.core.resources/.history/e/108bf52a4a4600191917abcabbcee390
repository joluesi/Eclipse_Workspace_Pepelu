matriz = [[1, 2, 3], [2, 12, 6], [1, 0, -3], [0,-1, 0]]

for fila in matriz:
    for j in fila:    
        print(j, end = "\t")
    print()
        
for i in range(len(matriz)):
    for j in range(len(matriz[i])):
        print(matriz[i][j], end = "\t")
    print()
a = [0] * 6
print(a)

c = [a] * 3
print(c)

c[0][0] = 1
print(c)

# lo mismo en un paso
M = [[0] * 6] * 3
print(M)
M[0][0] = 1
print(M)

matriz = []
for i in range(0,3):
    for j in range(0,3):
        #a = [0] * 6
        matriz.append(a)

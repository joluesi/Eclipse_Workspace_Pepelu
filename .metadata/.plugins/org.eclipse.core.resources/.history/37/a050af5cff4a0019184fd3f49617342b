package recuperación_01;

import java.io.File;

public class ejercicio_3 {
	
	public static void main(String[] args){
		
		File fichero = new File("/home/dam18-27/Descargas/libro.xsl");
		
		long ms = System.currentTimeMillis();
		System.out.printf("A día de hoy han pasado %d ms desde el 01-01-1970", ms);
		boolean cambio = fichero.setLastModified(ms);
		
		
	}

}
